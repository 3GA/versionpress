{
    "name": "versionpress/versionpress",
    "description": "VersionPress is a version control plugin for WordPress. It versions both files and the database enabling things like site-wide reverts, safe updates, easy staging etc. All on your server.",
    "require": {
        "tracy/tracy": "2.2.*",
        "nette/utils": "2.2.*",
        "nette/robot-loader": "2.2.*",
        "nette/neon": "2.2.*",
        "symfony/process": "2.5.*",
        "michelf/php-markdown": "1.4.*",
        "symfony/filesystem": "~2.5",
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-selenium": "1.4.0",
        "janvoracek/comment-stripper": "dev-master",
        "nette/database": "2.2.*"
    },
    "license": "GPL",
    "authors": [
        {
            "name": "VersionPress Team",
            "email": "info@versionpress.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/JanVoracek/nette-minified"
        }
    ]
}
